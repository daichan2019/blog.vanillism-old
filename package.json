{
  "name": "nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:aspida": "aspida --watch",
    "gen": "run-p gen:*",
    "gen:types": "npx microcms-typescript src/lib/cms/schema src/lib/cms/types/response.ts",
    "gen:aspida": "aspida",
    "dog:arr": "npx scaffdog generate api-array",
    "dog:obj": "npx scaffdog generate api-object",
    "clean": "rimraf .next out",
    "build": "aspida && next build",
    "export": "next export",
    "build:clean": "run-p clean build",
    "export:clean": "run-p clean build export",
    "start": "next start",
    "serve": "run-s -l -c build:clean start",
    "prepare": "husky install",
    "lint": "next lint --dir src",
    "prettier": "prettier --check .",
    "fix": "run-s -l -c fix:*",
    "fix:eslint": "eslint --fix .",
    "fix:prettier": "prettier --write ."
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 100,
    "tabWidth": 2
  },
  "dependencies": {
    "@aspida/fetch": "^1.11.0",
    "@headlessui/react": "^1.7.4",
    "base64url": "^3.0.1",
    "cheerio": "^1.0.0-rc.12",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.7",
    "highlight.js": "^11.6.0",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^7.0.0",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/line-clamp": "0.4.4",
    "@tailwindcss/typography": "0.5.9",
    "@types/cheerio": "^0.22.31",
    "@types/highlightjs": "^9.12.2",
    "@types/node": "18.15.11",
    "@types/react": "18.0.33",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "autoprefixer": "10.4.14",
    "eslint": "8.37.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import-access": "1.0.2",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "eslint-plugin-tailwindcss": "3.6.2",
    "eslint-plugin-testing-library": "5.10.2",
    "husky": "8.0.3",
    "lint-staged": "13.2.1",
    "microcms-typescript": "^1.0.13",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.21",
    "prettier": "2.8.7",
    "react-error-boundary": "4.0.3",
    "rimraf": "4.4.1",
    "scaffdog": "^2.4.0",
    "tailwindcss": "3.3.1",
    "typescript": "5.0.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix .",
      "prettier --write ."
    ],
    "**/*.{json,md,mdx,css,html,yml,yaml,scss}": [
      "prettier --write ."
    ]
  }
}
